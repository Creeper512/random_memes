{
    "version": 2,
    "functions": {
            "./api.php": {
                "runtime": "vercel-php@0.4.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/index.html",
            "dest": "/public/index.html"
        }
    ]
}